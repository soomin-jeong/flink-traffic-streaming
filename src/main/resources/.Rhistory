#Example 1. Bootstrapping proportions
# Simulate survey responses
survey<-c(rep(0,10),rep(1,15))
obs.mean<-mean(survey)
survey.boot.mean<- numeric(0)
survey.boot.sd<- numeric(0)
for (i in 1:1000) {
temp<- sample(survey,size=length(survey), replace=TRUE)
survey.boot.mean<-c(survey.boot.mean, mean(temp))
survey.boot.sd<-c(survey.boot.sd, sd(temp))
}
hist(survey.boot.mean)
for (i in 1:5000) {
temp<- sample(survey,size=length(survey), replace=TRUE)
survey.boot.mean<-c(survey.boot.mean, mean(temp))
survey.boot.sd<-c(survey.boot.sd, sd(temp))
}
mean(survey.boot.mean)
hist(survey.boot.mean)
# Confidence interval 2. The bootstrap confidence interval (based on a bootstrap standard error) for the mean is
#lower bound
mean(survey.boot.mean)-2*sd(survey.boot.mean)
# upper bound
mean(survey.boot.mean)+2*sd(survey.boot.mean)
# Confidence interval 3.The percentile method:
quantile(survey.boot.mean, probs=c(0.025, 0.975))
# Confidence interval 1. The pivot confidence interval.
# Lower limit
2*obs.mean-quantile(survey.boot.mean, probs=c(0.975))
# Upper limit
2*obs.mean-quantile(survey.boot.mean, probs=c(0.025))
